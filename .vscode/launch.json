    "version": "0.2.0",
    "configurations": [
        {
            "name": "FIRST_FIT 1024",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Log710Test",
            "args": [
                "-n",
                "1024",
                "-s",
                "f"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "[make] Log710Test",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
